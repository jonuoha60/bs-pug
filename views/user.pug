doctype html
html(lang="en")
  head
    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width, initial-scale=1.0")
    title Dashboard
  body
    if user
      h1 Welcome #{user.email}!
      p You are logged in.
    else
      h1 You are not logged in.
      a(href="/users/login") Go to Login Page
